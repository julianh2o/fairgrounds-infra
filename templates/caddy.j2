(standard) {
  log {
    output file /logs/logging.txt {
    }
    format json
    level info
  }
}

### Ansible Managed Proxies - Do not modify anything below this line ###
{% for name, service in services.services.items() %}
https://{{service.hostname}} {
  reverse_proxy http://{{ service.machine }}:{{ service.port }}
  import standard
}
{% endfor %}